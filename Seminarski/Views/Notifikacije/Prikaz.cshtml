@{
    ViewData["Tittle"] = "Prikaz";
}
@model IEnumerable<Seminarski.ViewModels.NotifikacijaPrikazVM>

<div class="table">
    <table>
        <tr>
            <th>Korisnik</th>
            <th>Poruka</th>
        </tr>
        <tbody>
            @foreach (var x in Model)
            {
                <tr>
                    <td>@x.ImePrezime</td>
                    <td>@x.Poruka</td>
                </tr>
            }

        </tbody>
    </table>
</div>
<style>
    body {
        /* background-color: #343a40;*/
        background-image: url("//archello.s3.eu-central-1.amazonaws.com/images/2018/03/22/---------------------------------------------.1521707101.6691.jpg");
        background-position: center;
        background-size: cover;
    }
    .table{
        color:white;
    }
    
</style>